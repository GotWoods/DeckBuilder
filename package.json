{
  "name": "deckbuilder-monorepo",
  "version": "1.0.0",
  "description": "DeckBuilder monorepo with separate client and server",
  "scripts": {
    "install:server": "cd server && npm install",
    "install:client": "cd client && npm install",
    "install:all": "npm run install:server && npm run install:client",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "build:all": "npm run build:server && npm run build:client",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "worker": "cd server && npm run worker",
    "test:server": "cd server && npm test",
    "test:all": "npm run test:server"
  },
  "keywords": ["deckbuilder", "react", "express"],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "server",
    "client"
  ]
}
